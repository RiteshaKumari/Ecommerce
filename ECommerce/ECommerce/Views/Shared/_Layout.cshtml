    <style>
        :root {
            --primary-bg-color: #f67981;
            --primary-bg-hover: #f67981;
        }

        .top1 {
            background-color: #fff;
            display: none;
        }

        @@media(max-width:767px) {
            .top1 {
                display: block;
            }
        }

        @@media(min-width:1199px) {
            .bhbutton {
                padding: 10px 20px !important;
            }
        }

        @@media(max-width:1199px) {
            .bhbutton {
                display: none !important;
            }
        }

        .dropdown-menu {
            display: none;
            position: absolute;
            top: 100%;
            left: 0;
            background-color: white;
            box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
            padding: 12px 0;
            z-index: 1000;
            min-width: 120px;
        }

            .dropdown-menu li {
                list-style: none;
                padding: 8px 16px;
            }

                .dropdown-menu li:hover {
                    color: #F67981;
                }

                .dropdown-menu li a {
                    text-decoration: none;
                    color: #333;
                    display: block;
                }

                    .dropdown-menu li a:hover {
                        /*background-color: #f1f1f1;*/
                        color: #F67981;
                    }

        .shopping-cart:hover .dropdown-menu {
            display: block; /* Show dropdown when hovering over profile link */
        }

        .profiledrop {
            display: flex;
            flex-wrap: nowrap;
            cursor: pointer;
        }

        .one-line-clamp {
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
            width: 241px;
        }
    </style>

</head>
