    <script>
function logout() {
    Swal.fire({
        title: "Are you sure?",
        text: "You want to Logout!",
        icon: "warning",
        showCancelButton: true,
        confirmButtonColor: "#3085d6",
        cancelButtonColor: "#d33",
        confirmButtonText: "Yes, Logout"
    }).then((result) => {
        if (result.isConfirmed) {
            $.ajax({
                url: '@Url.Action("CustomerLogout")',
                type: 'POST',
                success: function() {
                    Swal.fire({
                        position: "center",
                        icon: "success",
                        title: "You have been logged out",
                        showConfirmButton: false,
                        timer: 2000
                    }).then(() => {
                        // Optionally, you can refresh the page or perform any other actions
                        location.reload();
                    });
                },
                error: function() {
                    Swal.fire({
                        position: "center",
                        icon: "error",
                        title: "Something went wrong!",
                        showConfirmButton: false,
                        timer: 2000
                    });
                }
            });
        }
    });
}
    </script>
